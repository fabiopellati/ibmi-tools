{
  "name": "fabiopellati/ibmi-tools",
  "description": "IbmiToolkit's event-driven implementations based on MessageExchangeEventManager Actuator",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Fabio Pellati",
      "email": "fabio.pellati@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable":true,
  "require": {
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-eventmanager": "^3.1",
    "zendframework/zend-cache": "^2.7",
    "zendframework/zend-crypt": "^3.2",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-http": "^2.6",
    "zendtech/ibmitoolkit": "^1.7",
    "fabiopellati/message-exchange-event-manager": "^1.0 | dev-master"
  },
  "autoload": {
    "psr-4": {
      "IbmiTools\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  },
  "extra": {
    "zf": {
      "component": [
        "MessageExchangeEventManager"
      ]
    }
  },
  "require-dev": {
  }
